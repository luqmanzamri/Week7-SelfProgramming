package Selfprog;

class ex2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//declare dua variable x =0, y=1. Selagi y lebih kecil dan sama dgn 10,.
		//  tambah x dgn y. kemudian tambah y dgn 5. print x
		int x = 0;
		int y = 1;
		while (y <= 10) {
			x = x + y;
	        System.out.println(x + "");
	         y= y+5; 
	    }

	}

}
